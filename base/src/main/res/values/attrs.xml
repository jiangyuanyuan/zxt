<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--HeaderBar自定义属性-->
    <declare-styleable name="HeaderBar">
        <attr name="isShowLeftImg" format="boolean"/>
        <attr name="isShowRightImg" format="boolean"/>
        <attr name="isShowCancel" format="boolean"/>
        <attr name="rightImg" format="reference"/>
        <attr name="leftImg" format="reference"/>
        <attr name="titleText" format="string"/>
        <attr name="rightText" format="string"/>
        <attr name="bgColor" format="color"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>
</resources>
